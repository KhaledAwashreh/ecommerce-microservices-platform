{
  "info": {
    "name": "E-Commerce Microservices Platform",
    "description": "Postman collection for the E-Commerce Microservices Platform\n\nBase URL: http://localhost:8765 (API Gateway)\n\nServices:\n- API Gateway: http://localhost:8765\n- Naming Server (Eureka): http://localhost:8761\n- Config Server: http://localhost:8888",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8765",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "User Service",
      "item": [
        {
          "name": "Get All Users",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/user",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "user"]
            }
          }
        },
        {
          "name": "Get User By ID",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/user/:userId",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "user", ":userId"]
            }
          }
        },
        {
          "name": "Get User By Username",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/user?username={{username}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "user"],
              "query": [
                {
                  "key": "username",
                  "value": "{{username}}"
                }
              ]
            }
          }
        },
        {
          "name": "Register User",
          "request": {
            "method": "POST",
            "url": {
              "raw": "{{baseUrl}}/api/v1/user/register",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "user", "register"]
            },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"johndoe\",\n  \"email\": \"john@example.com\",\n  \"password\": \"password123\",\n  \"firstName\": \"John\",\n  \"lastName\": \"Doe\",\n  \"phoneNumber\": \"+1234567890\"\n}"
            }
          }
        },
        {
          "name": "Login User",
          "request": {
            "method": "POST",
            "url": {
              "raw": "{{baseUrl}}/api/v1/user/login",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "user", "login"]
            },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"username\": \"johndoe\",\n  \"password\": \"password123\"\n}"
            }
          }
        },
        {
          "name": "Delete User",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{baseUrl}}/api/v1/user/:userId",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "user", ":userId"]
            }
          }
        }
      ]
    },
